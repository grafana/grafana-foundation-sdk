{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "DataTopic": {
      "enum": [
        "series",
        "annotations",
        "alertStates"
      ],
      "description": "A topic is attached to DataFrame metadata in query results.\nThis specifies where the data should be used."
    },
    "DataSourceJsonData": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "authType": {
          "type": "string"
        },
        "defaultRegion": {
          "type": "string"
        },
        "profile": {
          "type": "string"
        },
        "manageAlerts": {
          "type": "boolean"
        },
        "alertmanagerUid": {
          "type": "string"
        }
      },
      "description": "TODO docs"
    },
    "DataQuery": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "refId"
      ],
      "properties": {
        "refId": {
          "type": "string",
          "description": "A unique identifier for the query within the list of targets.\nIn server side expressions, the refId is used as a variable name to identify results.\nBy default, the UI will assign A-\u003eZ; however setting meaningful names may be useful."
        },
        "hide": {
          "type": "boolean",
          "description": "If hide is set to true, Grafana will filter out the response(s) associated with this query before returning it to the panel."
        },
        "queryType": {
          "type": "string",
          "description": "Specify the query flavor\nTODO make this required and give it a default"
        },
        "datasource": {
          "type": "object",
          "additionalProperties": {},
          "description": "For mixed data sources the selected datasource is on the query level.\nFor non mixed scenarios this is undefined.\nTODO find a better way to do this ^ that's friendly to schema\nTODO this shouldn't be unknown but DataSourceRef | null"
        }
      },
      "description": "These are the common properties available to all queries in all datasources.\nSpecific implementations will *extend* this interface, adding the required\nproperties for the given context."
    },
    "BaseDimensionConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "field": {
          "type": "string",
          "description": "fixed: T -- will be added by each element"
        }
      }
    },
    "ScaleDimensionMode": {
      "enum": [
        "linear",
        "quad"
      ]
    },
    "ScaleDimensionConfig": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "min",
        "max"
      ],
      "properties": {
        "min": {
          "type": "number"
        },
        "max": {
          "type": "number"
        },
        "fixed": {
          "type": "number"
        },
        "field": {
          "type": "string",
          "description": "fixed: T -- will be added by each element"
        },
        "mode": {
          "$ref": "#/definitions/ScaleDimensionMode",
          "description": "| *\"linear\""
        }
      }
    },
    "ColorDimensionConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "fixed": {
          "type": "string",
          "description": "color value"
        },
        "field": {
          "type": "string",
          "description": "fixed: T -- will be added by each element"
        }
      }
    },
    "ScalarDimensionMode": {
      "enum": [
        "mod",
        "clamped"
      ]
    },
    "ScalarDimensionConfig": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "min",
        "max"
      ],
      "properties": {
        "min": {
          "type": "number"
        },
        "max": {
          "type": "number"
        },
        "fixed": {
          "type": "number"
        },
        "field": {
          "type": "string",
          "description": "fixed: T -- will be added by each element"
        },
        "mode": {
          "$ref": "#/definitions/ScalarDimensionMode"
        }
      }
    },
    "TextDimensionMode": {
      "enum": [
        "fixed",
        "field",
        "template"
      ]
    },
    "TextDimensionConfig": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "mode"
      ],
      "properties": {
        "mode": {
          "$ref": "#/definitions/TextDimensionMode"
        },
        "field": {
          "type": "string",
          "description": "fixed: T -- will be added by each element"
        },
        "fixed": {
          "type": "string"
        }
      }
    },
    "ResourceDimensionMode": {
      "enum": [
        "fixed",
        "field",
        "mapping"
      ]
    },
    "MapLayerOptions": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "type",
        "name"
      ],
      "properties": {
        "type": {
          "type": "string"
        },
        "name": {
          "type": "string",
          "description": "configured unique display name"
        },
        "config": {
          "type": "object",
          "additionalProperties": {},
          "description": "Custom options depending on the type"
        },
        "location": {
          "$ref": "#/definitions/FrameGeometrySource",
          "description": "Common method to define geometry fields"
        },
        "filterData": {
          "type": "object",
          "additionalProperties": {},
          "description": "Defines a frame MatcherConfig that may filter data for the given layer"
        },
        "opacity": {
          "type": "integer",
          "description": "Common properties:\nhttps://openlayers.org/en/latest/apidoc/module-ol_layer_Base-BaseLayer.html\nLayer opacity (0-1)"
        },
        "tooltip": {
          "type": "boolean",
          "description": "Check tooltip (defaults to true)"
        }
      }
    },
    "FrameGeometrySourceMode": {
