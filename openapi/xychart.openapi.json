{
  "openapi": "3.0.0",
  "info": {
    "title": "xychart",
    "version": "0.0.0",
    "x-schema-identifier": "xychart",
    "x-schema-kind": "composable",
    "x-schema-variant": "panelcfg"
  },
  "paths": {},
  "components": {
    "schemas": {
      "SeriesMapping": {
        "enum": [
          "auto",
          "manual"
        ],
        "description": "Auto is \"table\" in the UI"
      },
      "ScatterShow": {
        "enum": [
          "points",
          "lines",
          "points+lines"
        ]
      },
      "XYDimensionConfig": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "frame"
        ],
        "properties": {
          "frame": {
            "type": "integer",
            "minimum": 0
          },
          "x": {
            "type": "string"
          },
          "exclude": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "description": "Configuration for the Table/Auto mode"
      },
      "FieldConfig": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "show": {
            "$ref": "#/components/schemas/ScatterShow",
            "default": "points"
          },
          "pointSize": {
            "$ref": "#/components/schemas/ScaleDimensionConfig"
          },
          "pointColor": {
            "$ref": "#/components/schemas/ColorDimensionConfig"
          },
          "lineColor": {
            "$ref": "#/components/schemas/ColorDimensionConfig"
          },
          "lineWidth": {
            "type": "integer",
            "minimum": 0
          },
          "lineStyle": {
            "$ref": "#/components/schemas/LineStyle"
          },
          "label": {
            "$ref": "#/components/schemas/VisibilityMode",
            "default": "auto"
          },
          "hideFrom": {
            "$ref": "#/components/schemas/HideSeriesConfig"
          },
          "axisPlacement": {
            "$ref": "#/components/schemas/AxisPlacement"
          },
          "axisColorMode": {
            "$ref": "#/components/schemas/AxisColorMode"
          },
          "axisLabel": {
            "type": "string"
          },
          "axisWidth": {
            "type": "number"
          },
          "axisSoftMin": {
            "type": "number"
          },
          "axisSoftMax": {
            "type": "number"
          },
          "axisGridShow": {
            "type": "boolean"
          },
          "scaleDistribution": {
            "$ref": "#/components/schemas/ScaleDistributionConfig"
          },
          "axisCenteredZero": {
            "type": "boolean"
          },
          "labelValue": {
            "$ref": "#/components/schemas/TextDimensionConfig"
          },
          "axisBorderShow": {
            "type": "boolean"
          }
        }
      },
      "ScatterSeriesConfig": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "x": {
            "type": "string"
          },
          "y": {
            "type": "string"
          },
          "show": {
            "$ref": "#/components/schemas/ScatterShow",
            "default": "points"
          },
          "pointSize": {
            "$ref": "#/components/schemas/ScaleDimensionConfig"
          },
          "pointColor": {
            "$ref": "#/components/schemas/ColorDimensionConfig"
          },
          "lineColor": {
            "$ref": "#/components/schemas/ColorDimensionConfig"
          },
          "lineWidth": {
            "type": "integer",
            "minimum": 0
          },
          "lineStyle": {
            "$ref": "#/components/schemas/LineStyle"
          },
          "label": {
            "$ref": "#/components/schemas/VisibilityMode",
            "default": "auto"
          },
          "hideFrom": {
            "$ref": "#/components/schemas/HideSeriesConfig"
          },
          "axisPlacement": {
            "$ref": "#/components/schemas/AxisPlacement"
          },
          "axisColorMode": {
            "$ref": "#/components/schemas/AxisColorMode"
          },
          "axisLabel": {
            "type": "string"
          },
          "axisWidth": {
            "type": "number"
          },
          "axisSoftMin": {
            "type": "number"
          },
          "axisSoftMax": {
            "type": "number"
          },
          "axisGridShow": {
            "type": "boolean"
          },
          "scaleDistribution": {
            "$ref": "#/components/schemas/ScaleDistributionConfig"
          },
          "axisCenteredZero": {
            "type": "boolean"
          },
          "name": {
            "type": "string"
          },
          "labelValue": {
            "$ref": "#/components/schemas/TextDimensionConfig"
          },
          "axisBorderShow": {
            "type": "boolean"
          }
        }
      },
      "Options": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "dims",
          "legend",
          "tooltip",
          "series"
        ],
        "properties": {
          "seriesMapping": {
            "$ref": "#/components/schemas/SeriesMapping"
          },
          "dims": {
            "$ref": "#/components/schemas/XYDimensionConfig",
            "description": "Table Mode (auto)"
          },
          "legend": {
            "$ref": "#/components/schemas/VizLegendOptions"
          },
          "tooltip": {
            "$ref": "#/components/schemas/VizTooltipOptions"
          },
          "series": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ScatterSeriesConfig"
            },
            "description": "Manual Mode"
          }
        }
      },
      "ScaleDimensionConfig": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "min",
          "max"
        ],
        "properties": {
          "min": {
            "type": "number"
          },
          "max": {
            "type": "number"
          },
          "fixed": {
            "type": "number"
          },
          "field": {
            "type": "string",
            "description": "fixed: T -- will be added by each element"
          },
          "mode": {
            "$ref": "#/components/schemas/ScaleDimensionMode",
            "description": "| *\"linear\""
          }
        }
      },
      "ColorDimensionConfig": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "fixed": {
            "type": "string",
            "description": "color value"
          },
          "field": {
            "type": "string",
            "description": "fixed: T -- will be added by each element"
          }
        }
      },
      "LineStyle": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "fill": {
            "enum": [
              "solid",
              "dash",
              "dot",
              "square"
            ]
          },
          "dash": {
            "type": "array",
            "items": {
              "type": "number"
            }
          }
        },
        "description": "TODO docs"
      },
      "VisibilityMode": {
        "enum": [
          "auto",
          "never",
          "always"
        ],
        "description": "TODO docs"
      },
      "HideSeriesConfig": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "tooltip",
          "legend",
          "viz"
        ],
        "properties": {
          "tooltip": {
            "type": "boolean"
          },
          "legend": {
            "type": "boolean"
          },
          "viz": {
            "type": "boolean"
          }
        },
        "description": "TODO docs"
      },
      "AxisPlacement": {
        "enum": [
          "auto",
          "top",
          "right",
          "bottom",
          "left",
          "hidden"
        ],
        "description": "TODO docs"
      },
      "AxisColorMode": {
        "enum": [
          "text",
          "series"
        ],
        "description": "TODO docs"
      },
      "ScaleDistributionConfig": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "type"
        ],
        "properties": {
          "type": {
            "$ref": "#/components/schemas/ScaleDistribution"
          },
          "log": {
            "type": "number"
          },
          "linearThreshold": {
            "type": "number"
          }
        },
        "description": "TODO docs"
      },
      "TextDimensionConfig": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "mode"
        ],
        "properties": {
          "mode": {
            "$ref": "#/components/schemas/TextDimensionMode"
          },
          "field": {
            "type": "string",
            "description": "fixed: T -- will be added by each element"
          },
          "fixed": {
            "type": "string"
          }
        }
      },
      "VizLegendOptions": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "displayMode",
          "placement",
          "showLegend",
          "calcs"
        ],
        "properties": {
          "displayMode": {
            "$ref": "#/components/schemas/LegendDisplayMode",
            "default": "list"
          },
          "placement": {
            "$ref": "#/components/schemas/LegendPlacement",
            "default": "bottom"
          },
          "showLegend": {
            "type": "boolean"
          },
          "asTable": {
            "type": "boolean"
          },
          "isVisible": {
            "type": "boolean"
          },
          "sortBy": {
            "type": "string"
          },
          "sortDesc": {
            "type": "boolean"
          },
          "width": {
            "type": "number"
          },
          "calcs": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "default": []
          }
        },
        "description": "TODO docs"
      },
      "VizTooltipOptions": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "mode",
          "sort"
        ],
        "properties": {
          "mode": {
            "$ref": "#/components/schemas/TooltipDisplayMode"
          },
          "sort": {
            "$ref": "#/components/schemas/SortOrder"
          }
        },
        "description": "TODO docs"
      },
      "ScaleDimensionMode": {
        "enum": [
          "linear",
          "quad"
        ]
      },
      "ScaleDistribution": {
        "enum": [
          "linear",
          "log",
          "ordinal",
          "symlog"
        ],
        "description": "TODO docs"
      },
      "TextDimensionMode": {
        "enum": [
          "fixed",
          "field",
          "template"
        ]
      },
      "LegendDisplayMode": {
        "enum": [
          "list",
          "table",
          "hidden"
        ],
        "description": "TODO docs\nNote: \"hidden\" needs to remain as an option for plugins compatibility"
      },
      "LegendPlacement": {
        "enum": [
          "bottom",
          "right"
        ],
        "description": "TODO docs"
      },
      "TooltipDisplayMode": {
        "enum": [
          "single",
          "multi",
          "none"
        ],
        "description": "TODO docs"
      },
      "SortOrder": {
        "enum": [
          "asc",
          "desc",
          "none"
        ],
        "description": "TODO docs"
      }
    }
  }
}