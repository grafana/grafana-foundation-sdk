

# EditorConfig is awesome: https://editorconfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

[*.py]
indent_style = space
indent_size = 4

[*.ts]
indent_style = space
indent_size = 2

[*.php]
indent_style = space
indent_size = 4

[*.go]
indent_style = tab
